  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "eslint": "^4.9.0",
    "express": "^4.16.3",
    "express-openapi-validate": "^0.4.4",
    "http": "0.0.0",
    "marked": "^0.5.1",
    "memory-cache": "^0.2.0",
    "mongoose": "^5.3.8",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-strategy": "^1.0.0",
    "request": "^2.88.0",
    "swagger-ui-express": "^4.0.1",
    "url": "^0.11.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint-config-airbnb-base": "^12.1.0",
